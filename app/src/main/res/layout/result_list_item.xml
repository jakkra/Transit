<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
                                    android:orientation="horizontal" android:layout_width="match_parent"
                                    xmlns:card_view="http://schemas.android.com/apk/res-auto"
                                    android:layout_marginBottom="2dp"
                                    android:layout_marginTop="2dp"
                                    android:layout_marginLeft="4dp"
                                    android:id="@+id/resultCardItem"
                                    android:layout_marginRight="4dp"
                                    android:layout_gravity="center"
                                    card_view:cardCornerRadius="2dp"
                                    android:layout_height="120dp"
                                    android:clickable="true"
                                    android:foreground="?android:attr/selectableItemBackground">

    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/resultListTextColor"
                android:id="@+id/transport_name_nbr"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" android:textStyle="bold" android:text="Stadsbuss 4"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/resultListTextColor"
                android:id="@+id/time_between" android:layout_below="@+id/transport_name_nbr"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" android:textSize="18sp" android:text="20:37 - 20:41"/>
        <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/ic_bus_local"

                android:id="@+id/imageView1"
                android:layout_above="@+id/textView" android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"/>
        <TextView
                android:layout_width="wrap_content"
                android:textColor="@color/resultListTextColor"
                android:layout_height="wrap_content"
                android:id="@+id/nbr_dep_time"
                android:textStyle="bold" android:text="19 min" android:layout_above="@+id/message"
                android:layout_alignParentRight="true" android:layout_alignParentEnd="true"/>
        <View style="@style/DividerResult"
              android:padding="5dp"
              android:layout_above="@+id/message" android:layout_alignParentLeft="true"
              android:layout_alignParentStart="true" android:id="@+id/textView"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/resultListTextColor"
                android:id="@+id/message"
                android:layout_alignParentBottom="true" android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:text="Varning Text Text TextText Text TextText Text TextText Text TextText Text TextTText Text TextText Text Textext Text Text"
                android:textSize="10sp"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/resultListTextColor"
                android:text="om 2 min"
                android:id="@+id/time_to_dep" android:layout_alignParentTop="true" android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"/>
        <ImageView android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/ic_bus_local"
                   android:id="@+id/iw_first_bus_type" android:layout_toEndOf="@+id/transport_name_nbr"
                   android:layout_alignBottom="@+id/transport_name_nbr"
                   android:layout_toRightOf="@+id/transport_name_nbr"/>
        <ImageView android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/ic_clock_red"
                   android:id="@+id/iw_clock" android:layout_toEndOf="@+id/time_between"
                   android:layout_alignTop="@+id/time_between" android:layout_toRightOf="@+id/time_between"/>
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                  android:textColor="@color/resultListTextColor" android:text="1 min" android:id="@+id/textView2"
                  android:layout_toEndOf="@+id/iw_clock"
                  android:layout_alignBottom="@+id/iw_clock" android:layout_toRightOf="@+id/iw_clock"/>
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                  android:textColor="@color/resultListTextColor" android:text="3" android:id="@+id/textView4"
                  android:layout_alignTop="@+id/imageView1" android:layout_toRightOf="@+id/imageView1"
                  android:layout_toEndOf="@+id/imageView1"/>
        <ImageView android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/ic_next"
                   android:id="@+id/imageView2"
                   android:layout_toEndOf="@+id/textView4" android:layout_above="@+id/textView"
                   android:layout_toRightOf="@+id/textView4"/>
        <ImageView android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/ic_bus_region"
                   android:id="@+id/imageView3"
                   android:layout_above="@+id/textView" android:layout_toRightOf="@+id/imageView2"
                   android:layout_toEndOf="@+id/imageView2"/>
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                  android:textColor="@color/resultListTextColor" android:text="3" android:id="@+id/textView5"
                  android:layout_toEndOf="@+id/imageView3"
                  android:layout_alignTop="@+id/imageView3" android:layout_toRightOf="@+id/imageView3"/>
        <ImageView android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/ic_next"
                   android:id="@+id/imageView4"
                   android:layout_toEndOf="@+id/textView5"
                   android:layout_alignTop="@+id/textView5" android:layout_toRightOf="@+id/textView5"/>
        <ImageView android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/ic_bus_local"
                   android:id="@+id/imageView5"
                   android:layout_toEndOf="@+id/imageView4"
                   android:layout_above="@+id/textView" android:layout_toRightOf="@+id/imageView4"/>
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                  android:textColor="@color/resultListTextColor" android:text="3" android:id="@+id/textView7"
                  android:layout_toEndOf="@+id/imageView5"
                  android:layout_alignTop="@+id/imageView5" android:layout_toRightOf="@+id/imageView5"/>
        <ImageView android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/ic_next"
                   android:id="@+id/imageView6" android:layout_toEndOf="@+id/textView7"
                   android:layout_above="@+id/textView" android:layout_toRightOf="@+id/textView7"/>
        <ImageView android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/ic_train_pagatag"
                   android:id="@+id/imageView7" android:layout_toEndOf="@+id/imageView6"
                   android:layout_above="@+id/textView" android:layout_toRightOf="@+id/imageView6"/>
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                  android:textColor="@color/resultListTextColor" android:text="3" android:id="@+id/textView8"
                  android:layout_toEndOf="@+id/imageView7"
                  android:layout_alignTop="@+id/imageView7" android:layout_toRightOf="@+id/imageView7"/>
        <ImageView android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/ic_next"
                   android:id="@+id/imageView8" android:layout_toEndOf="@+id/textView8"
                   android:layout_above="@+id/textView" android:layout_toRightOf="@+id/textView8"/>
        <ImageView android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/ic_bus_local"
                   android:id="@+id/imageView9" android:layout_toEndOf="@+id/imageView8"
                   android:layout_above="@+id/textView" android:layout_toRightOf="@+id/imageView8"/>
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                  android:textColor="@color/resultListTextColor" android:text="3" android:id="@+id/textView9"
                  android:layout_toEndOf="@+id/imageView9"
                  android:layout_alignBottom="@+id/textView8" android:layout_toRightOf="@+id/imageView9"/>
        <ImageView android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/ic_next"
                   android:id="@+id/imageView10" android:layout_toEndOf="@+id/textView9"
                   android:layout_above="@+id/textView" android:layout_toRightOf="@+id/textView9"/>
        <ImageView android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/ic_train_oresund"
                   android:id="@+id/imageView11" android:layout_toEndOf="@+id/imageView10"
                   android:layout_above="@+id/textView" android:layout_toRightOf="@+id/imageView10"/>
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                  android:textColor="@color/resultListTextColor" android:text="2 byten" android:id="@+id/textView10"
                  android:layout_below="@+id/time_between" android:layout_alignParentLeft="true"
                  android:layout_alignParentStart="true"/>

    </RelativeLayout>
</android.support.v7.widget.CardView>